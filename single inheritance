import java.io.*;
import java.util.*;
public class inheritance
{
		public void m1()
		{
				System.out.println("parent class");
		}
}
class b extends inheritance
{
		public void m2()
		{
				System.out.println("child class");
		}
}
class c
{
		public static void main(String args[])
		{
				b B = new b();
				B.m1();
				B.m2();
		}
}
